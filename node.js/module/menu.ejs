<div id="clock">
      <span id="nowTimes">
        <a href="" id="realtime" style="margin-bottom: 5.4%;"></a>
      </span>
    </div>
    
    <div class="navname">
      <% if (login) { %>
        <a href="/profile" class="navdata">
          <%- name %>님
          <% if (admin) { %>
            <br>(관리자)
            <style>
              .navname {
                font-size: 1.6vh;
                top: 2vh;
              }
            </style>
          <% } %>
        </a>
      <% } else { %>
        <a href="/login" class="navdata">로그인</a>
      <% } %>
    </div>
    <div class="toggle">
      <ion-icon name="menu-outline" class="open"></ion-icon>
      <ion-icon name="close-outline" class="close"></ion-icon>
    </div>
    <div class="nav">
      <ul>
        <!--
          <li class="list<%- (active == 'EXAMPLE') ? ' active' : '' %>"><b></b><b></b>
            <a href="/EXAMPLE">
              <span class="icon"></span>
              <span class="title">EXAMPLE</span>
            </a>
          </li>
        -->
        <li class="list<%- (active == '홈') ? ' active' : '' %>"><b></b><b></b>
          <a href="/">
            <span class="icon"><ion-icon name="home-outline"></ion-icon></span>
            <span class="title">홈</span>
          </a>
        </li>
        <% if (login) { %>
          <li class="list<%- (active == '내정보') ? ' active' : '' %>"><b></b><b></b>
            <a href="/profile">
              <span class="icon"><ion-icon name="person-circle-outline"></ion-icon></span>
              <span class="title">내정보</span>
            </a>
          </li>
          <li class="list<%- (active == '자가진단') ? ' active' : '' %>"><b></b><b></b>
            <a href="/selfcheck">
              <span class="icon"><ion-icon name="shield-checkmark-outline"></ion-icon></span>
              <span class="title">자가진단</span>
            </a>
          </li>
          <% if (admin) { %>
            <li class="list<%- (active == '관리') ? ' active' : '' %>"><b></b><b></b>
              <a href="/manage">
                <span class="icon"><ion-icon name="hammer-outline"></ion-icon></span>
                <span class="title">관리</span>
              </a>
            </li>
          <% } %>
          <li class="list<%- (active == '설정') ? ' active' : '' %>"><b></b><b></b>
            <a href="/setting">
              <span class="icon"><ion-icon name="settings-outline"></ion-icon></span>
              <span class="title">설정</span>
            </a>
          </li>
          <li class="list<%- (active == '로그아웃') ? ' active' : '' %>"><b></b><b></b>
            <a href="/logout">
              <span class="icon"><ion-icon name="log-out-outline"></ion-icon></span>
              <span class="title">로그아웃</span>
            </a>
          </li>
        <% } else { %>
          <li class="list<%- (active == '로그인') ? ' active' : '' %>"><b></b><b></b>
            <a href="/login">
              <span class="icon"><ion-icon name="log-in-outline"></ion-icon></span>
              <span class="title">로그인</span>
            </a>
          </li>
          <li class="list<%- (active == '회원가입') ? ' active' : '' %>"><b></b><b></b>
            <a href="/signup">
              <span class="icon"><ion-icon name="person-add-outline"></ion-icon></span>
              <span class="title">회원가입</span>
            </a>
          </li>
        <% } %>
      </ul>
    </div>